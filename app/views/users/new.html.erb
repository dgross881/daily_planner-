<%= title "Sign up" %>
<%= form_for(@user, builder: FoundationFormBuilder) do |f| %>
      <%= f.text_field :first_name, label: "First Name", auto_focus: "auto_focus" %>
      <%= f.text_field :last_name %>
      <%= f.text_field :email %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation, label: "Password(again)" %>
     <%= f.submit "Sign Up", class: "button radius expand" %>
<% end %>
